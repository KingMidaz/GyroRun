config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[android]

modules="org/godotengine/godot/FireBase,org/godotengine/godot/SQLBridge"

[application]

config/name="GyroRun"
run/main_scene="res://MainMenu/MainMenu.tscn"
boot_splash/image="res://ArtAssets/MainMenu.png"
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://ArtAssets/GyroRun Logo.png"

[autoload]

global="*res://global.gd"

[display]

window/size/width=1126
window/size/height=2436
window/size/borderless=true
window/size/fullscreen=true
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/aspect="keep_width"
window/stretch/shrink="1"

[rendering]

environment/default_environment="res://default_env.tres"
